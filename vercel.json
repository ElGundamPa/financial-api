{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python@3.1.10",
      "config": {
        "pythonVersion": "3.10"
      }
    },
    {
      "src": "api/receiver.py",
      "use": "@vercel/python@3.1.10",
      "config": {
        "pythonVersion": "3.10"
      }
    }
  ],
  "routes": [
    { "src": "/api/index", "dest": "api/index.py" },
    { "src": "/api/index/(.*)", "dest": "api/index.py" },
    { "src": "/api/receiver", "dest": "api/receiver.py" },
    { "src": "/api/receiver/(.*)", "dest": "api/receiver.py" }
  ]
}


