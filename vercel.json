{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.10"
    }
  },
  "routes": [
    { "src": "/api/index(?:/.*)?", "dest": "api/index.py" },
    { "src": "/api/receiver(?:/.*)?", "dest": "api/receiver.py" }
  ]
}

{
  "version": 2,
  "regions": ["gru1"],
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    { "src": "/api/receiver(?:/.*)?", "dest": "api/receiver.py" },
    { "src": "/(.*)", "dest": "api/index.py" }
  ]
}

