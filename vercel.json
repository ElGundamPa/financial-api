{
  "functions": {
    "api/index.py": {
      "runtime": "vercel-python@3.11"
    },
    "api/receiver.py": {
      "runtime": "vercel-python@3.11"
    }
  },
  "regions": ["gru1"],
  "routes": [
    { "src": "/api/receiver(.*)", "dest": "/api/receiver.py" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}
